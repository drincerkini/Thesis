@model Branch

<h1>Departments of @Model.Name</h1>

<table class="table table-light table-striped">
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var enrollment in Model.DeptBranches)
        {
            <tr>
                <td>@enrollment.Department.Name</td>
            </tr>
        }
    </tbody>
</table>
<div>
    <button class="btn btn btn-light">
        <a class="btn btn-ligt text-secondary" asp-action="Index">Back to List</a>
    </button>
</div>
